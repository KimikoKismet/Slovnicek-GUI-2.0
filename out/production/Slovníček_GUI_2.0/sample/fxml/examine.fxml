<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="Pain" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.examine.ExamineController">
   <children>
      <Label layoutX="20.0" layoutY="216.0" text="Words left:" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="250.0">
